{
  "name": "lucide-react-sprite",
  "version": "2.1.0",
  "private": false,
  "description": "A high-performance icon system for React applications that uses lucide-react components during development and generates a single, optimized SVG sprite for production builds.",
  "keywords": [
    "react",
    "typescript",
    "icons",
    "svg",
    "sprite",
    "lucide",
    "lucide-react"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "homielab",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/homielab/lucide-react-sprite.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "lucide-sprite": "./dist/cli.js"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "development": "./dist/dev.mjs",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "development": "./dist/dev.js",
        "default": "./dist/index.js"
      }
    }
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "glob": "^12.0.0",
    "lucide-react": "^0.554.0",
    "lucide-static": "^0.554.0",
    "svgo": "^4.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "jsdom": "^27.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.11"
  }
}
